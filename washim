    CustomLog /home/washim/logs/ combined
    <Directory "/home/washim/public_html/dev-nbsols-washim">
       Options +Indexes FollowSymlinks
       AllowOverride All
       Order allow,deny
       Allow from all
    </Directory>
</VirtualHost>
